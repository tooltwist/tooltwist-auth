<% errorMessages.forEach(function(e){ %>
  <div class="container pages alert alert-danger">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <%= e %>
  </div>
<% });%>

<% successMessages.forEach(function(e){ %>
  <div class="container pages alert alert-success">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <%= e %>
  </div>
<% });%>